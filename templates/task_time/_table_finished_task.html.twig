<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Start date</th>
        <th scope="col">End date</th>
        <th scope="col">Total time</th>
    </tr>
    </thead>
    <tbody>

    {% for task_time in finished_task_times %}

        <tr>
            <th scope="row">{{ task_time.id() }}</th>
            <td>{{ task_time.startDate()|date('Y-m-d H:i:s') }}</td>
            <td>{{ task_time.endDate()|date('Y-m-d H:i:s') }}</td>
            <td>{{ task_time.intervalTaskTime() }} Hours</td>
        </tr>
    {% endfor %}

    </tbody>
</table>
<p>Total hours: {{ amount_hours_task_time }}</p>